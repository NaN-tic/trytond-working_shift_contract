<form col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="contract"/>
    <field name="contract"/>
    <label name="sequence"/>
    <field name="sequence"/>
    <newline/>
    <label name="start_date"/>
    <field name="start_date"/>
    <label name="end_date"/>
    <field name="end_date"/>
    <group colspan="3" col="2" id="conditions" string="Conditions"
            yexpand="1" yfill="1">
        <label name="hours"/>
        <field name="hours"/>
    </group>
    <group colspan="3" col="2" id="result" string="Result"
            yexpand="1" yfill="1">
        <label name="product"/>
        <field name="product"/>
        <label name="list_price"/>
        <field name="list_price"/>
    </group>
</form>
